[alias]
	a    = add
	b    = branch
	c    = config
	ci   = commit
	co   = checkout
	d    = diff
	f    = fetch
	i    = init
	l    = log
	m    = mv
	r    = rebase
	s    = status
	st   = stash
	sub  = submodule
	pick = cherry-pick
[user]
	name = Keith Noguchi
	email = hack@noguchi.us
[push]
	default = simple
[include]
	path = ~/.gnupg/secret.txt
[core]
	excludesfile = ~/.gitignore_global
[init]
	defaultBranch = main
